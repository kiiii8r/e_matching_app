<%= render "shared/contents-header" %>

<div class='main-contents'>
  <div class='contents'>

    <div class='account-page user-form'>
      <%= form_with model: @profile, url: user_profile_path(params[:user_id]), local: true do |f| %>

        <%= render 'profile', f: f %>

        <%= f.submit "編集を完了する" ,class:"registration-btn" %>
        
      <% end %>
    </div>
  </div>
</div>

<%= render "shared/contents-footer" %>